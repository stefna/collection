{
	"name": "stefna/ds-collection",
	"description": "Common collection classes for stefna",
	"type": "library",
	"minimum-stability": "dev",
	"prefer-stable": true,
	"license": "MIT",
	"authors": [
		{
			"name": "Andreas Sundqvist",
			"email": "andreas@stefna.is"
		}
	],
	"require": {
		"php": "^8.2",
		"ext-ds": "*"
	},
	"require-dev": {
		"squizlabs/php_codesniffer": "^3.9",
		"stefna/codestyle": "^1.15",
		"phpstan/phpstan": "^1.10",
		"bnf/phpstan-psr-container": "^1.0",
		"phpunit/phpunit": "^10.0",
		"phpstan/phpstan-phpunit": "^1.1",
		"tomasvotruba/type-coverage": "^0.0.7",
		"phpstan/extension-installer": "^1.3"
	},
	"autoload": {
		"psr-4": {
			"Stefna\\Collection\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Stefna\\Collection\\Tests\\": "tests/"
		}
	},
	"config": {
		"allow-plugins": {
			"phpstan/extension-installer": true
		}
	}
}
